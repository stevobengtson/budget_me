<li class="sub-pill<%= active_transaction_link?(account.id) %>" id="account_nav_link_<%= account.id %>">
  <ul class="nav nav-pills nav-edit">
    <li class="account-link<%= active_transaction_link?(account.id) %>"><%= link_to account.name, transactions_url(account_id: account.id) %></li>
    <li class="pull-right">
      <%= link_to(account_url(id: account.id), method: :delete, class: 'account-delete', data: {accountid: account.id, confirm: 'Are you sure?'}) do %>
        <i class="fa fa-trash-o text-danger" ></i>
      <% end %>
    </li>
  </ul>
</li>
